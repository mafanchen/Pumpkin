        <item name="android:layout_height">@dimen/setPhone_input_normal_height</item>
        <item name="android:background">@null</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/signup_font_select_country</item>
        <item name="android:textColorHint">@color/signup_font_hint_color</item>
        <item name="android:textSize">@dimen/text_size_14sp</item>

    </style>

    <style name="setPhone_selectCountry">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/setPhone_input_normal_height</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/setPhone_font_title</item>
        <item name="android:textSize">@dimen/text_size_16sp</item>
        <item name="android:layout_marginStart">@dimen/login_padding_input_left_right</item>
    </style>

    <style name="setEmail_email" parent="setPhone_selectCountry">

    </style>


    <style name="video_popup_toast_anim" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>

    <style name="bean_video_style_dialog_progress" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/video_popup_toast_anim</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="bean_video_vertical_progressBar">
