<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<WebView
		android:id="@+id/wv_webProgram"
		android:layout_width="match_parent"
		android:layout_height="match_parent" />

	<LinearLayout
		android:layout_width="87dp"
		android:layout_height="32dp"
		android:layout_gravity="end"
		android:layout_marginTop="12dp"
		android:layout_marginEnd="12dp"
		android:background="@drawable/shape_bg_btn_webprogram"
		android:orientation="horizontal">

		<ImageView
			android:id="@+id/iv_edit_webProgram"
			android:layout_width="43dp"
			android:layout_height="match_parent"
			android:scaleType="center"
			android:src="@drawable/ic_edit_nav_web" />

		<View
			android:layout_width="1dp"
			android:layout_height="19dp"
			android:layout_gravity="center_vertical"
			android:background="@color/colorWhite" />

		<ImageView
			android:id="@+id/iv_close_webProgram"
			android:layout_width="43dp"
			android:layout_height="match_parent"
			android:scaleType="center"
			android:src="@drawable/ic_close_nav_web" />

	</LinearLayout>

</FrameLayout>